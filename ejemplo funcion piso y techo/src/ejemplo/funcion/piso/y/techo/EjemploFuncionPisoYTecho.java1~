package ejemplo.funcion.piso.y.techo;

/**
 *
 * @author MADARME
 */
public class EjemploFuncionPisoYTecho {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
       
        float x;
        int f;
        for(float i=101;i<=103;i++)
        {
        
            double res1=((i-3)/4);
            float res2=((i-1)/100);
            int techo=(int)Math.ceil(res2);
            int piso=(int)Math.floor(res1);
            System.out.println("f("+i+")="+i+"-"+res1+"+"+res2);
            f=(int)(i-piso+techo);
            System.out.println("f("+i+")="+i+"-"+piso+"+"+techo+"="+f);
            
            System.out.println();
            
            
        
        }
        
        
        
        
        
    }
}
